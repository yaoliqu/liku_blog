{
  "name": "liku_blog",
  "version": "0.0.1",
  "private": false,
  "description": "liku blog front",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "build_dev": "vite build --mode=dev",
    "build_prod": "vite build --mode=prod",
    "build_test": "vite build --mode=test",
    "serve": "vite preview",
    "serve_dev": "vite serve --mode=dev --host 0.0.0.0",
    "serve_prod": "vite serve --mode=prod",
    "serve_test": "vite serve --mode=test",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://https://github.com/yaoliqu/liku_blog"
  },
  "keywords": [],
  "author": "liku",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yaoliqu/liku_blog/issues"
  },
  "homepage": "https://github.com/yaoliqu/liku_blog#readme",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.21.1",
    "css-vars-ponyfill": "^2.4.7",
    "highlight.js": "^11.1.0",
    "js-md5": "^0.7.3",
    "marked": "^2.1.3",
    "moment": "^2.29.1",
    "mutationobserver-shim": "^0.3.7",
    "pinia": "^2.0.11",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/js-md5": "^0.4.3",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^2.0.0-rc.4",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylus": "^0.54.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "unplugin-vue-components": "^0.17.11",
    "vite": "^2.5.1",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.3.0"
  }
}
